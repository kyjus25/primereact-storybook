{
  "compilerOptions": {
    "target": "es2016",                                 
    "module": "ES2022",
    "moduleResolution": "Node",
    "resolveJsonModule": true,                 
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,  
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["gen"],
  "exclude": [
    "node_modules",
  ]
}
